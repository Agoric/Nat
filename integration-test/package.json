{
  "name": "integration-test",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "private": true,
  "scripts": {
    "test": "tape -r esm test/*.js",
    "test:pre-release": "tape -r esm test/test-pre-release.js",
    "test:post-release": "tape -r esm test/test-post-release.js",
    "create-test-file-no-nat-cjs": "rollup -c transform-tests/config/rollup.config.no-nat.js",
    "create-test-file-esm": "rollup -c transform-tests/config/rollup.config.esm.js",
    "create-test-file-cjs": "rollup -c transform-tests/config/rollup.config.cjs.js",
    "create-test-file-browserified-tape": "browserify transform-tests/output/test.no-nat.cjs.js > transform-tests/output/test.tape-no-nat.js --exclude @agoric/nat",
    "build:webpack": "webpack -r esm --display-error-details --config scaffolding/webpack/webpack.config.js",
    "build:browserify": "browserify transform-tests/output/test.cjs.js > bundles/browserify.js",
    "build:rollup": "rollup -c scaffolding/rollup/rollup.config.test.js",
    "build:parcel": "parcel build scaffolding/parcel/index.html --public-url ./ -d bundles/parcel"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@agoric/nat": "file:agoric-nat-2.0.1.tgz"
  },
  "devDependencies": {
    "browserify": "^16.2.3",
    "esm": "^3.2.11",
    "parcel": "^1.11.0",
    "puppeteer-core": "^1.13.0",
    "rollup": "^1.4.1",
    "rollup-plugin-node-resolve": "^4.0.1",
    "rollup-plugin-replace": "^2.1.0",
    "tape": "^4.10.1",
    "tape-promise": "^4.0.0",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.2.3"
  }
}
